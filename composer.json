{
    "type": "project",
    "license": "proprietary",
    "version": "dev-main",
    "repositories": [
        {
            "type": "package",
            "package": {
                "type": "metapackage",
                "name": "vendor/package-patches",
                "version": "1.0.0",
                "require": {
                    "netresearch/composer-patches-plugin": "~1.0"
                },
                "extra": {
                    "patches": {
                        "doctrine/orm": {
                            "^2.11": [
                                {
                                    "url": "./patches/doctrine_nullable_embeddables.patch",
                                    "title": "[FEATURE] Nullable Embeddables for Doctrine 2"
                                }
                            ]
                        }
                    }
                }
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-event": "*",
        "ext-redis": "*",
        "ext-uuid": "*",
        "ext-amqp": "^2.0",
        "doctrine/orm": "^2.11",
        "doctrine/dbal": "^3.2",
        "vlucas/phpdotenv": "^5.2",
        "workerman/workerman": "^4.1",
        "twig/twig": "^3.0",
        "guzzlehttp/guzzle": "^7.3",
        "http-interop/http-factory-guzzle": "^1.0",
        "workerman/crontab": "^1.0",
        "monolog/monolog": "^2.6",
        "laminas/laminas-crypt": "^3.8",
        "firebase/php-jwt": "^6.9",
        "vendor/package-patches": "~1.0",
        "symfony/console": "^6.1",
        "wedrix/watchtower": "^5.0",
        "laravel/serializable-closure": "^1.3",
        "matthiasmullie/scrapbook": "^1.5",
        "symfony/var-exporter": "^6.2",
        "symfony/mailer": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App"
        },
        "files": [
            "src/array_extensions.php",
            "src/string_extensions.php",
            "src/App/Console/AddPluginWatchtowerCommand.php",
            "src/App/Console/AddScalarTypeDefinitionWatchtowerCommand.php",
            "src/App/Console/GenerateCacheWatchtowerCommand.php",
            "src/App/Console/GenerateSchemaWatchtowerCommand.php",
            "src/App/Console/ListPluginsWatchtowerCommand.php",
            "src/App/Console/ListScalarTypeDefinitionsWatchtowerCommand.php",
            "src/App/Console/UpdateSchemaWatchtowerCommand.php",
            "src/App/DataStore/NamingStrategy.php",
            "src/App/DataStore/UnderscoreNamingStrategy.php",
            "src/App/Server/RequestHandler/AccountOfUser.php",
            "src/App/Server/RequestHandler/RequestOfUser.php",
            "src/App/Server/RequestHandler/Session.php",
            "src/App/Server/RequestHandler/SessionOfUser.php",
            "src/App/Server/RequestHandler/User.php",
            "src/App/Server/RequestHandler/UserOfRequest.php",
            "src/App/Server/RequestHandler/UserOfSession.php",
            "src/App/Server/RequestHandler/WindowAccess.php",
            "src/App/Server/ConfiguredJobs.php",
            "src/App/Server/Logger.php",
            "src/App/Server/ProcessTaskQueueIndefinitelyJob.php",
            "src/App/Server/RequestHandler.php",
            "src/App/Account.php",
            "src/App/Cache.php",
            "src/App/Config.php",
            "src/App/Console.php",
            "src/App/DataStore.php",
            "src/App/Email.php",
            "src/App/EmailClient.php",
            "src/App/Encrypter.php",
            "src/App/Hasher.php",
            "src/App/Server.php",
            "src/App/SymfonyMailer.php",
            "src/App/TaskQueue.php",
            "src/App/TwigTemplateEngine.php",
            "src/App/WatchtowerConsole.php",
            "src/App/WatchtowerExecutor.php",
            "src/Inflector.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "fakerphp/faker": "^1.13",
        "phpunit/phpunit": "^9.5",
        "phrozenbyte/phpunit-array-asserts": "^1.2",
        "doctrine/data-fixtures": "^1.5",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phploc/phploc": "^7.0",
        "sebastian/phpcpd": "^6.0"
    },
    "config": {
        "allow-plugins": {
            "netresearch/composer-patches-plugin": true,
            "php-http/discovery": true
        }
    }
}
